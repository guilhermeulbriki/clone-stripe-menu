{
    "env": {
        "browser": true,
        "es2020": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/jsx-filename-extension": "off",
        "import/prefer-default-export": "off",
        "react/button-has-type": "off",
        "react/prop-types": "off",
        "no-param-reassign": "off",
        "consistent-return": "off",
        "no-plusplus": "off",
        "no-return-assign": "off"
    }
}
